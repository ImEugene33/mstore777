<link href="{{ 'styles.css' | asset_url }}" rel="stylesheet" type="text/css">
<script src="{{ 'custom-instagram.js' | asset_url }}" defer="defer"></script>

<instagram-gallery class="p-5" user-id="{{ section.settings.userId }}" access-token="{{ section.settings.token }}"></instagram-gallery>

{% schema %}
{
  "name": "Insta",
  "settings": [
    {
      "type": "textarea",
      "id": "token",
      "label": "Access Token",
    },
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Logo",
      "info": "Hero Image"
    },
    {
      "type": "text",
      "id": "userId",
      "label": "User ID",
    },
  ],
  "presets": [
    {
      "name": "Insta"
    }
  ]
}
{% endschema %}